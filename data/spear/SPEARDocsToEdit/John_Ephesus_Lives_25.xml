                <listPerson>
                    <person>
                        <persName ref="http://syriaca.org/person/784"/>
                        <occupation ref="http://syriaca.org/keyword/bishops">
                            <note type="desc">
                                <persName ref="http://syriaca.org/person/784">Theodosius</persName>was archbishop of Alexandria.
                            </note>
                        </occupation>
                    </person>
                </listPerson>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/817"/>
                    <citedRange unit="pp">326</citedRange>

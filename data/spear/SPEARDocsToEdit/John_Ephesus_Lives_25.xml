                    <citedRange unit="pp">527</citedRange>
                </bibl>
            </div>
            <div uri="http://syriaca.org/spear/817-8" resp="#syoung">
                <listPerson>
                    <person>
                        <persName ref="http://syriaca.org/person/1524">Z'ura</persName>
                    </person>
                </listPerson>
                <bibl type="primary">

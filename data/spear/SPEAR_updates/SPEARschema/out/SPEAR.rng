         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-xmlid-divFactoid-ids-constraint-assert-66">
            <rule context="tei:div/@xml:id">
               <sch:assert xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           test="count(distinct-values(//tei:div/@xml:id)) eq count(//tei:div/@xml:id)">Each &lt;div&gt; element @uri attribute must end with a unique number following
                    the "-".</sch:assert>
            </rule>
         </pattern>
         <optional>
            <attribute name="resp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <data type="token">
                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>

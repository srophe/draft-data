         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-atts-on-factoidDiv-constraint-rule-53">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:srophe="https://srophe.app"
                      context="tei:div[ancestor::tei:body]">
                <sch:assert test="@xml:id">Elements of type &lt;div&gt; within the &lt;body&gt; element
                  require an @xml:id attribute.</sch:assert>
                <sch:assert test="@resp">Elements of type &lt;div&gt; within the body element
                  require a @resp attribute.</sch:assert>
                <sch:assert test="@type='factoid'">Elements of type &lt;div&gt; within the body element
                require a @type attribute with the value "factoid".</sch:assert>
              </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-atts-on-factoidDiv-constraint-rule-54">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:srophe="https://srophe.app"
                      context="tei:div[ancestor::tei:body]/@resp">
                <sch:let name="edsDoc"
                        value="doc('https://raw.githubusercontent.com/srophe/srophe-eXist-app/master/documentation/editors.xml')"/>
                <sch:let name="eds"
                        value="$edsDoc//tei:text/tei:body/tei:listPerson/tei:person/@xml:id"/>
                <sch:let name="respValues" value="for $i in $eds return concat('#', $i)"/>
                <sch:assert test="                   every $i in .                   satisfies $i = $respValues">
                  Acceptable values for this @resp attribute include: 
                  <sch:value-of select="string-join($respValues, ' | ')"/>.
                </sch:assert>
              </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-abstractModel-structure-l-constraint-report-11">
            <rule context="tei:div">
               <report xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                       xmlns:srophe="https://srophe.app"
                       test="ancestor::tei:l">
        Abstract model violation: Lines may not contain higher-level structural elements such as div.
      </report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-abstractModel-structure-p-constraint-report-12">
            <rule context="tei:div">
               <report xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                       xmlns:srophe="https://srophe.app"
                       test="ancestor::tei:p or ancestor::tei:ab and not(ancestor::tei:floatingText)">
        Abstract model violation: p and ab may not contain higher-level structural elements such as div.
      </report>
            </rule>
         </pattern>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <data type="ID"/>
            </attribute>
         </optional>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-xmlid-divFactoid-ids-constraint-assert-64">
            <rule context="tei:div/@xml:id">
               <sch:assert xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           test="starts-with(., 'factoid-')">An @xml:id attribute
                    starts with 'factoid-'.</sch:assert>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-xmlid-divFactoid-ids-constraint-report-13">
            <rule context="tei:div/@xml:id">
               <sch:report xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           test="matches(substring-after(., '-'), '\D')">A properly formatted
                    SPEAR factoid URI ends with a hyphen followed by a number.</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="SPEAR-div-xmlid-divFactoid-ids-constraint-assert-65">
            <rule context="tei:div/@xml:id">
               <sch:assert xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           test="matches(substring-after(., '-'), '\d')">A properly formatted

               <value>attestation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An event that attests to the entity described in the document</a:documentation>
               <value>other</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Any event that does something other than attest to the entity described in the document</a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="where">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the location of an event by pointing to a <code xmlns="http://www.w3.org/1999/xhtml">&lt;place&gt;</code> element</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tei_geo">
